<%= render "layouts/partials/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cost %>
  <%= f.number_field :cost, step: 0.01 %>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <%= f.label :product_image %>
  <%= f.file_field :product_image %>
  <%= f.submit %>
<% end %>
